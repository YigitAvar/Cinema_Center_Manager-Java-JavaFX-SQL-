        <ComboBox fx:id="revenueIdComboBox" layoutX="150.0" layoutY="60.0" prefHeight="26.0" prefWidth="166.0" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="60.0" />
        <Button fx:id="refundButton" layoutX="150.0" layoutY="100.0" stylesheets="@../css/Admin.css" text="Refund Product" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="100.0">
